{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ES2018", "ES2019.Object", "ES2019.Array", "ES2020.String"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"types": ["vite/client", "node"],
		"jsx": "react"
	},
	"include": ["src/**/*"]
}
